{
  "name": "@i18n-micro/types-generator",
  "version": "1.0.1",
  "description": "Type generator for i18n-micro translation keys",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "i18n",
    "typescript",
    "type-generator",
    "translation"
  ],
  "author": {
    "name": "s00d",
    "email": "Virus191288@gmail.com",
    "url": "https://s00d.github.io/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s00d/nuxt-i18n-micro"
  },
  "dependencies": {
    "globby": "^14.1.0",
    "chokidar": "^3.6.0",
    "unplugin": "^1.16.1"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.26",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "typescript": "^5.9.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "@types/jest": "^29.5.14"
  }
}

